{
    "name": "reactreduxexpress",
    "version": "1.0.0",
    "description": "Boilerplate for react redux and express",
    "main": "server.js",
    "engines": {
        "node": "12.16.1",
        "npm": "6.14.5"
    },
    "scripts": {
        "client-install": "cd client npm install",
        "build": "cd client npm run build",
        "heroku-postbuild": "cd client && npm install && npm run build",
        "start": "node server.js",
        "server": "nodemon server.js --ignore './client/'",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "author": "Min He",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^4.0.1",
        "concurrently": "^5.2.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.1",
        "helmet": "^3.22.0",
        "mongodb": "^3.5.7",
        "node-fetch": "^2.6.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^9.8.0",
        "nodemon": "^2.0.3"
    }
}
